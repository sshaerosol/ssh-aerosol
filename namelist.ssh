/

&physic_gas_chemistry
tag_chem = 0,			
attenuation = 1.d0,               
with_photolysis = 1			
with_heterogeneous = 1,                 
with_adaptive = 1,                      
adaptive_time_step_tolerance = 0.001,    
min_adaptive_time_step = 0.6,    	
/

&physic_particle_numerical_issues
DTAEROMIN = 1E-15,              	!coag
redistribution_method = 0,   		!coag 0 10
with_fixed_density = 1,       		!coag
fixed_density = 1.84D-06,        	!coag
/

&physic_coagulation
with_coag = 1,                 		!coag
i_compute_repart = 0,                   ! 0 if repartition coeff are not computed but read from file, 1 if they are 
Coefficient_file = "coef_s1_f1_b50.nc",	!coag
Nmc = 10000
/

&physic_condensation
with_cond = 1,               		!coag
Cut_dim = 0.0,             
ISOAPDYN = 1,
nlayer = 1,
with_kelvin_effect = 1,                 ! 1 if kelvin effect is taken into account.
tequilibrium = 0.1,                     ! time under which equilibrium is assumed.
dorg = 1.d-12,                          ! diffusion coefficient in the organic phase.
coupled_phases = 1,                       ! 1 if aqueous and organic phases are coupled
activity_model = 1,               ! 1. "ideal", 2. "unifac", 3. "aiomfac"
/

&physic_nucleation
with_nucl = 0,                
nucl_model = 0,               
/

&physic_organic
with_oligomerization = 1      
/

&output
output_directory = "results", 
output_type = 1               
/
