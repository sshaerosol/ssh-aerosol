&emissions
tag_emis = 0,                
with_emis_num = 0,    
emis_gas_file = "./inputs/emis_gas.dat",
emis_aero_mass_file = "./inputs/emis_aero.dat", 
emis_aero_num_file = "./inputs/ext_num.init10e5.dat" 
/

&mixing_state
tag_external = 0,             
N_groups = 1,                  
N_frac = 1, 	                  
kind_composition = 1,          
/

&fraction_distribution
frac_input= 0.0 1.0,          
/

&gas_phase_species
species_list_file = "./species-list/species-list-cb05en.dat"               
/

&aerosol_species
aerosol_species_list_file = "./species-list/species-list-aer-en.dat"      
mineral_dust = "PMD",
black_carbon = "PBC",
isorropia_species_name = "PNA", "PSO4", "PNH4", "PNO3","PHCL",
aec_species_name = "PBiA2D", "PBiA1D", "PBiA0D", "PAGLY", "PAMGLY","PBiMT","PBiPER", "PBiDER", "PBiMGA", "PAnBlP", "PAnBmP", "PBiBlP", "PBiBmP","PBiNGA", "PBiNIT3", "PBiNIT", "PMonomer", "PDimer", "PBiA3D",
